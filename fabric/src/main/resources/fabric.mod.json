{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${mod_version}",
    "name": "${mod_name}",
    "description": "${mod_desc}",
    "authors": [ "${mod_author}" ],
    "contact": {
        "homepage": "${mod_homepage}",
        "sources": "${mod_source}",
        "issues": "${mod_issues}"
    },
    "license": "${mod_license}",
    "icon": "assets/${mod_id}/icon.png",
    "environment": "*",
    "entrypoints": {
        "client": [
            "com.mrcrayfish.catalogue.Catalogue"
        ]
    },
    "depends": {
        "fabricloader": ">=${fabric_loader_version}",
        "fabric": "*",
        "minecraft": "${minecraft_version}",
        "java": ">=17"
    },
    "custom": {
        "configured": {
            "configs": [],
            "background": "minecraft:textures/block/dirt.png"
        },
        "catalogue": {
            "banner": "assets/${mod_id}/banner.png",
            "background": "assets/${mod_id}/background.png"
        }
    }
}
  